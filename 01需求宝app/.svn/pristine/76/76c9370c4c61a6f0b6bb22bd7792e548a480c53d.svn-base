body,html {
	background: #EEE;
}
header {
	width: 100%;
	height: 44px;
}
.header_div{
	position : relative;
	width: 100%;
	height: 44px;
	line-height: 44px;
	border-bottom : 1px solid #eee;
}
.c_back_btn {
	position: absolute;
	top: 0px;
	left : 0px;
	width: 65px;
	height: 44px;
	line-height: 44px;
	text-align: center;
	font-size : 16px;
	color : #000000;
}
.c_confirm_btn{
	position: absolute;
	top: 0px;
	right: 0px;
	width : 65px;
	height: 44px;
	line-height: 44px;
	text-align: center;
	font-size : 16px;
	color : #ff6026;
}
