/*公用*/
.list-flex {
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
}
.list-flex-v {
	-webkit-box-orient: vertical;
	-ms-flex-flow: column;
	    flex-flow: column;
}

.list-flex-h {
	-webkit-box-orient: horizontal;
	-ms-flex-flow: row;
	    flex-flow: row;
}

.list-flex-flex1 {
	-webkit-box-flex: 1;
	-ms-flex: 1;
	    flex: 1;
}
.list-flex-center {
	-webkit-box-align: center;
	-ms-flex-align: center;
	align-items: center;
}
.line2 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	word-wrap:break-word;
}
.line1 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
}
#search-wrap .header {
	height: 1.173333rem;
	line-height: 1.173333rem;
	padding: 0  0.400000rem;
	border-bottom: 1px solid #eee;
}
#search-wrap .header .search-btn {
	height: 1.173333rem;
	font-size: 0.453333rem;
	color: #999999;
	margin-left: 0.400000rem;
}
#search-wrap .header .back {
	width: 0.640000rem;
	height: 1.173333rem;
	background-image: url(../image/label/back.png);
	background-repeat: no-repeat;
	background-position: center;
	background-size: 0.640000rem 0.640000rem;
	margin-right: 0.080000rem;
}
#search-wrap .header .search {
	height: 0.800000rem;
	line-height: 0.800000rem;
	background: #EEEEEE;
	border-radius: 2.666667rem;
	padding-left: 1.333333rem;
	background-image: url(../image/home/search_icon.png);
	background-position: 0.266667rem center;
	background-repeat: no-repeat;
	background-size: 0.480000rem 0.480000rem;
}
#search-wrap .header .search_spea {
	padding-left: 0.800000rem;
	font-size: 0.373333rem;
	color: #666;
}
.search-frame {
	background: #eee;
}
.hot-search {
	background: #fff;
	margin-bottom: 0.213333rem;
	padding: 0.346667rem 0.426667rem 0.053333rem;
	padding-right: 0px;
}
.search-title {
	font-size: 0.373333rem;
	color: #333333;
	line-height: 1;
}
.hot-search .search-title {
	margin-bottom: 0.373333rem;
}
.hot-label {
	display: inline-block;
	width: 2.880000rem;
	padding: 0.186667rem 0;
	text-align: center;
	font-size: 0.373333rem;
	color: #999999;
	border: 1px solid #EEEEEE;
	border-radius: 0.533333rem;
	margin-bottom: 0.266667rem;
	margin-right: 0.133333rem;
}
.history-search {
	background: #fff;
	padding: 0  0.426667rem;
}
.history-search .search-title {
	height: 0.986667rem;
	line-height: 0.986667rem;
}
.history-search .search-title .delete-btn {
	width: 0.640000rem;
	height: 0.640000rem;
	background-image: url(../image/home/Delete.png);
	background-repeat: no-repeat;
	background-size: 0.640000rem 0.640000rem;
	background-position: center;
}
.history-search .history-content {
	margin-top: 0.320000rem;
	font-size: 0.373333rem;
	color: #666666;
}
.history-search .history-content .item {
	height: 1.120000rem;
}
.history-null img {
	width: 3.306667rem;
	height: 2.586667rem;
}
.history-null {
	text-align: center;
	padding-top: 2.133333rem;
}
.null-alert {
	text-align: center;
	font-size: 0.373333rem;
	line-height: 0.533333rem;
	color: #999999;
	margin-top: 0.186667rem;
}
/*弹窗*/
.delete-model {
	height: 100%;
	background: rgba(0,0,0,0.70);
}
.delete-model .delete-content {
	width: 8.106667rem;
	height: 3.413333rem;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	background: #fff;
	border-radius: 0.213333rem;
}
.delete-model .delete-content .delete-title {
	height: 2.133333rem;
	font-size: 0.426667rem;
	color: #333333;
	text-align: center;
	-webkit-box-pack: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	border-top-left-radius: 0.213333rem;
	border-top-right-radius: 0.213333rem;
}
.delete-model .delete-content .delete-btn .btn {
	height: 1.280000rem;
	line-height: 1.280000rem;
	font-size: 0.453333rem;
	color: #999999;
	text-align: center;
	border-top: 1px solid #e4e4e4;
	border-bottom-left-radius: 0.213333rem;
}
.delete-model .delete-content .delete-btn .btn.active {
	color: #FFFFFF;
	background: #FF6026;
	border-top-color: transparent;
	border-bottom-left-radius: 0px;
	border-bottom-right-radius: 0.213333rem;
}
/*搜索圈子*/
