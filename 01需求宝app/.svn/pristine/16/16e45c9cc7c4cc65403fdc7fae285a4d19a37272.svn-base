/*公用*/
.list-flex{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.list-flex-v {
    -webkit-box-orient: vertical;
    -webkit-flex-flow: column;
    flex-flow: column;
}

.list-flex-h {
    -webkit-box-orient: horizontal;
    -webkit-flex-flow: row;
    flex-flow: row;
}

.list-flex-flex1 {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.list-flex-center{
  -webkit-box-align: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.line2 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
    word-wrap:break-word;
}
.line1 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
    word-wrap:break-word;
}

#search-wrap .header{
  height: 44px;
  line-height: 44px;
  padding: 0 15px;
  border-bottom: 1px solid #eee;
}
#search-wrap .header .search-btn{
  height: 44px;
  font-size: 17px;
  color: #999999;
  margin-left: 15px;
}
#search-wrap .header .back{
  width: 24px;
  height: 44px;
  background-image: url(../../image/label/back.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 24px 24px;
  margin-right: 3px;
}
#search-wrap .header .search{
  height: 30px;
  line-height: 32px;
  background: #EEEEEE;
  border-radius: 100px;
  padding-left: 30px;
  font-size: 14px;
  color: #666;
  background-image: url(../../image/home/search_icon.png);
  background-position: 10px center;
  background-repeat: no-repeat;
  background-size: 18px 18px;
}
.mean_list{
  border-bottom: 1px solid #eee;
}
.listname{
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: center;
}
.list_bor{
  border-right: 1px solid #EEE;
  width: 100%;
  font-size: 14px;
  line-height: 20px;
  color: #666;
}
.name_active{
  color: #FF6026;
}
.listname:last-child .list_bor{
  border-right: none;
}
.subbtn_footer{
  padding-right: 16px;
  padding-left: 16px;
  padding-bottom: 10px;
}
.subbtn{
  width: 100%;
  border-radius: 25px;
  background-color: #FF4400;
  color: #fff;
  font-size: 18px;
  text-align: center;
  line-height: 50px;
}
.select_tit{
	padding-left : 15px;
	padding-top: 12px;
	padding-bottom: 5px;
	line-height: 20px;
	font-size: 14px;
	color: #666;
}
.title{
	position: absolute;
	left : 0;
	top : 0;
	width: 100%;
	height: 44px;
	text-align: center;
	font-size: 16px;
	visibility: hidden;
}