.box {
    width: 26.66666667rem;
    white-space:normal; word-break:break-all;
}

.tab ul {
    margin: 1.70666667rem 1.13777778rem 1.10222222rem 1.13777778rem;
    overflow: hidden;
    width: 26.66666667rem;
    font-size: .99555556rem;
    font-family: PingFang-SC-Medium;
    color: #a6a6a6
}

.tab ul li {
    float: left;
    padding: 0 1.42222222rem 0 0
}

.tab p img {
    height: 9.6rem
}

.need {
    width: 24.24888889rem;
    height: 3.2rem;
    margin: 0 auto;
    margin-top: 1.35111111rem;
    font-size: .99555556rem;
    font-family: PingFang-SC-Bold;
    border: 1px solid #ebebeb;
    border-radius: .35555556rem;
    overflow: hidden;
    padding: 0 0 0 3rem;
    background: #fff;
    color: #a6a6a6
}

.need img {
    width: 1.31555556rem;
    height: 1.35111111rem;
    margin-right: .53333333rem
}

.need div {
    float: left;
    height: 1.42222222rem;
    margin: .81777778rem 0
}

.need div:nth-child(1) {
    padding: 0 2.77333333rem 0 .5rem;
    border-right: 2px solid #ebebeb
}

.need div:nth-child(2) {
    padding-left: 3.69777778rem
}

.content .contents {
    min-height: 8.28444444rem;
    background: #fff;
    padding: 1.13777778rem 0;
    border-bottom: .17777778rem solid #f8f8f8
}

.content .contents a {
    color: #1e1e1e
}

.content .contents img {
    width: 7.11111111rem;
    height: 7.11111111rem
}

.content .contents div {
    margin-left: 1.13777778rem
}

.content .contents div:nth-child(1) {
    overflow: hidden;
    margin-bottom: .71111111rem
}

.content .contents div:nth-child(1) p {
    width: 1.38666667rem;
    float: left;
    font-size: .99555556rem;
    font-family: ArialMT;
    color: #f63
}

.content .contents div:nth-child(1) h3 {
    width: 21.68888889rem;
    float: left;
    font-size: 1.13777778rem;
    color: #1e1e1e;
    font-weight: 600;
    font-family: PingFang-SC-Bold
}

.content .contents div:nth-child(2) {
    margin: 1.24444444rem 1.13777778rem 1.24444444rem 1.13777778rem;
    display: flex;
    justify-content: space-between
}

.content .contents div:nth-child(3) {
    margin-left: 2.34666667rem
}

.content .contents div:nth-child(3) span:nth-child(1) {
    color: #69f
}

.content .contents div:nth-child(3) span {
    font-size: .85333333rem;
    font-family: PingFang-SC-Medium;
    color: #b1b1b1
}

.content .contents div:nth-child(3) .hd {
    font-size: .85333333rem;
    font-family: PingFang-SC-Medium;
    color: #69f
}

.xq {
    height: 2.66666667rem;
