ul {
    margin: 0;
    padding: 0
}

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
    display: block
}

html {
    -webkit-text-size-adjust: none;
    -ms-text-size-adjust: none
}

body,
div,
fieldset,
form,
h1,
h2,
h3,
h4,
h5,
h6,
html,
p {
    -webkit-text-size-adjust: none
}

li,
ol,
ul {
    list-style: none outside none
}

img {
    width: 100%;
    border: 0 none;
    vertical-align: middle
}

input,
select {
    vertical-align: middle
}

table {
    border-collapse: collapse;
    border-spacing: 0
}

address,
caption,
cite,
code,
dfn,
em,
optgroup,
th,
var {
    font-style: inherit;
    font-weight: inherit
}
*{
      -webkit-user-select:text;
    -moz-user-select:text;
    -ms-user-select:text;
    user-select:text;
  }
h1,
h2,
h3,
h4,
h5,
h6 {
    font-size: 100%;
    font-weight: 400
}

abbr,
acronym {
    border: 0;
    font-variant: normal
}

button,
input,
optgroup,
option,
textarea {
    font-family: inherit;
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit
}

input[type=password],
input[type=text],
textarea {
    outline-style: none;
    -webkit-appearance: none
}

textarea {
    resize: none
}

button,
input,
select,
textarea {
    color: #000;
    border: 0 none;
    background: #fff
}

a,
button,
input,
select,
textarea {
    outline: 0
}

input::-moz-focus-inner {
    border: 0
}

input {
    resize: none;
    -webkit-appearance: none
}

a,
button,
img,
input,
select,
span {
    -webkit-tap-highlight-color: transparent;
    -webkit-tap-highlight-color: transparent
}

