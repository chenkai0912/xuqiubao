*,
:after,
:before {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box
}

body {
    font-size: .85333333rem;
    line-height: 1.5;
    font-family: arial, "Hiragino Sans GB", "microsoft yahei", georgia, verdana, helvetica, sans-serif;
    text-align: justify;
    text-justify: inter-ideograph;
    color: #000;
    background: 0 0;
    white-space:normal; word-break:break-all;
}
sort
article,
aside,
body,
dd,
details,
dl,
dt,
figcaption,
figure,
footer,
form,
h1,
h2,
h3,
h4,
h5,
h6,
header,
hgroup,
html,
label,
li,
menu,
nav,
ol,
p,
section,
table,
td,
