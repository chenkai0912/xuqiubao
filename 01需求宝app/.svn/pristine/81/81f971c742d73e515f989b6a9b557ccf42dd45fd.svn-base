}



.appointmentBox {
    background: #fff;
    height: 6.75rem;
    width: 100%;
     /*padding:0 0.85rem;*/
    position: relative;


}

.appointmentBox .appointment {
    height: 6.75rem;
    width: 100%;
    background: url("../images/bannersApponintement.png") no-repeat center  center;
    background-size: 100%;
}


.appointmentBox img{
    display: inline-block;
    position: absolute;
    width: 8.65rem;
    height: 1.65rem;
    left: 48%;
    transform: translateX(-50%);
    bottom: 16%;
}
.banner img{width:100%}
.list,.nav,.tops{padding:10px 0;background-color:#fff;margin-top:10px}
.nav li{float:left;width:20%;text-align:center;font-size:12px;line-height:30px;color:#666}
.nav li a{color:#666}
.nav img{width:60%;min-height:30px;display:block;margin:0 auto}
.clear{display:block;clear:both}
.list h3,.tops h3{font-size:16px;font-weight:700;padding:0 10px;margin-bottom:5px;color:#000;border-bottom:1px solid #eee;line-height:30px}
.tops ul{margin:0 10px;width:100%}
.tops li{float:left;width:30%;margin:0 3px;font-size:12px;position:relative; min-height: 10rem;}
.tops li img{width:100%}
.tops div{position:absolute;padding:5px;bottom:0;background-color:#000;opacity:.8;color:#fff}
.tops h4{font-weight:400;height:30px;overflow:hidden}
.tops p{font-size:12px;color:#999;padding-top:5px}
.tops p img{width:25px;height:25px;border-radius:30px}
 
