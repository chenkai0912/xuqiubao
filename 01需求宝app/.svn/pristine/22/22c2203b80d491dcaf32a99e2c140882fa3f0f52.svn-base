body, html {
  background: #fff;
  font-size: 20px;
}

header {
  width: 100%;
  height: 2.20rem;
  box-sizing: border-box;
  font-size: 0.85rem;
  color: #666;
  .header_title {
    position: relative;
    width: 100%;
    border-bottom: 1px solid #eee;
    box-sizing: border-box;
    .c_back_btn, .c_complete_btn, .c_who_btn {
      position: absolute;
      top: 0;
      line-height: 2.10rem;
      text-align: center;
    }

    .c_back_btn {
      left: 0.5rem;
    }

    .c_complete_btn {
      right: 0.5rem;
      color: #FF6026;
    }

    .c_who_btn {
      left: 50%;
      transform: translateX(-50%);
      color: #000;
    }

  }

}





