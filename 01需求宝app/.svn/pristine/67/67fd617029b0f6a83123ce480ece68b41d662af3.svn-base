body, html {
	background: #FFF;
}
header {
	width: 100%;
	height: 44px;
	line-height: 44px;
	background: #FFF;
}
.rank_content{
	position: relative;
	width: 100%;
	height: 44px;
	line-height: 44px;
	font-size:17px;
	text-align: center;
	border-bottom: 1px solid #EEEEEE;
}
.rank_back{
	position: absolute;
	left: 16px;
	top: 7px;
	width: 24px;
	height: 24px;
}
.meui_click{
	opacity: 0.7;
}
.slh{
	position: absolute;
	right: 16px;
	top: 10px;
	font-size: 0;
	width: 24px;
	height: 24px;
}
.slh img{
	width: 100%;
	height: 100%;
}
