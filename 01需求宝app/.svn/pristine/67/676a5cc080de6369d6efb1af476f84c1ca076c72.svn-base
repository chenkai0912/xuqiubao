<!DOCTYPE HTML>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="maximum-scale=1.0, minimum-scale=1.0, user-scalable=0, initial-scale=1.0, width=device-width" />
		<meta name="format-detection" content="telephone=no, email=no, date=no, address=no">
		<title>打赏列表</title>
		<link rel="stylesheet" type="text/css" href="../../css/api.css" />
		<link rel="stylesheet" type="text/css" href="../../css/ranking_list_win.css" />
		<script type="text/javascript" src="../../script/flexible.js"></script>
		<style>
			#header {
				border-bottom: 1px solid #eee
			}
		</style>
	</head>
	<body>
		<header id="header" class="rank_header">
			<p class="rank_content"><img class="rank_back" src="../../image/rank/rank_back.png" tapmode onclick="fnBack()">打赏列表
			</p>
		</header>
	</body>
	<script type="text/javascript" src="../../script/api.js"></script>
	<script type="text/javascript" src="../../script/mapi.js"></script>
	<script type="text/javascript" src="../../script/vue.js"></script>
	<script type="text/javascript">
		var myVue = null;
		apiready = function() {
			//fnRefreshOpen();
			api.setFullScreen({
				fullScreen : false
			});
			var header = $api.byId('header');
			fnSysHeaderStyleById();
			var headerH = header.offsetHeight;
			api.openFrame({
				name : 'reward_list_frame',
				url : 'reward_list_frame.html?v=' + new Date().getTime(),
				rect : {
					x : 0,
					y : headerH,
					w : 'auto',
					h : 'auto'
				},
				pageParam : {
					storyId : api.pageParam.storyId
				}
			});
			fnBackListener(fnBack)
		};
	</script>
</html>
