body, html {
	background: #FFF;
}
header {
	width: 100%;
	height: 44px;
	line-height: 44px;
	background: #FFF;
}
.rank_content{
	position: relative;
	width: 100%;
	height: 44px;
	line-height: 44px;
	font-size:17px;
	text-align: center;
	border-bottom: 1px solid #EEEEEE;
}
.rank_back{
	position: absolute;
	left: 16px;
	top: 7px;
	width: 24px;
	height: 24px;
}
.rule{
	position: absolute;
	right: 16px;
	line-height: 44px;
	font-size: 12px;
	color: #666;
}
.tabs{
	padding-left: 16px;
	padding-right: 16px;
	display: flex;
}
.tab{
	flex: 1;
	font-size: 16px;
	color: #999;
	padding-top: 11px;
	padding-bottom: 9px;
	height: 22px;
	line-height: 22px;
	text-align: center;
	border-bottom: 2px solid #EEE;
}
.tab_active{
	border-color: #FF6026;
	color: #FF6026;
}
.meui_click{
	opacity: 0.7;
}
