body,html{
	background-color: #eee;
	overflow-x: hidden;
}
.banner{
	width: 100%;
	height: auto;
	font-size: 0;
}
.banner img{
	width: 100%;
	height: 130px;
}
.business_tabs{
	width : 100%;
	height : 48px;
	background-color: #fff;
	padding: 11px 0px 9px;
	padding-left : 16px;
	box-sizing: border-box;
}
.tabs_padd{
	padding-top: 5px;
	padding-bottom: 5px;
	padding-right: 16px;
	border-right: 1px solid #eee;
}
.whole{
	background: #FF6026;
	width: 44px;
	text-align: center;
	height: 20px;
	line-height: 20px;
	font-size: 12px;
	color: #FFF;
	border-radius: 4px;
}
.two_button{
	height : 48px;
	padding-top: 5px;
	padding-bottom: 5px;
	box-sizing : border-box;
	font-size: 0;
}
.buttonone{
	width: 88px;
	text-align: center;
	height: 20px;
	line-height: 20px;
	font-size: 12px;
	color: #333;
	border: 1px solid #EEE;
	border-radius: 100px;
	margin-left: 16px;
}