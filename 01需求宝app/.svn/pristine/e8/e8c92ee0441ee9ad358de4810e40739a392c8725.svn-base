/*公用*/

.list-flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}

.list-flex-v {
	-webkit-box-orient: vertical;
	-webkit-flex-flow: column;
	flex-flow: column;
}

.list-flex-h {
	-webkit-box-orient: horizontal;
	-webkit-flex-flow: row;
	flex-flow: row;
}

.list-flex-flex1 {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
}

.list-flex-center {
	-webkit-box-align: center;
	-ms-flex-align: center;
	align-items: center;
}

.line1 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
    word-wrap:break-word;
}

.line2 {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
    word-wrap:break-word;
}

