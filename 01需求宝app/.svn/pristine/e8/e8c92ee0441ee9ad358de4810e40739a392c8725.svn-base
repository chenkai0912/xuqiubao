.talk {
	box-sizing: border-box;
	width: 191px;
	/*height: 32px;*/
	background: #EEEEEE;
	border: 1px solid #E4E4E4;
	border-radius: 10px;
	padding-top: 8px;
	padding-bottom: 7px;
	padding-left: 39px;
	background-image: url(../image/mine/write@2x.png);
	background-repeat: no-repeat;
	background-size: 24px 24px;
	background-position: 14px;
	font-size: 12px;
	color: #999999;
	min-height: 32px;
	max-height: 300px;
	word-wrap: break-word;
	overflow-x: hidden;
	overflow-y: visible;
	outline: none;
	-webkit-user-modify: read-write-plaintext-only;
}
#footer .operation {
	height: 32px;
	margin-left: 12px;
}

#footer .operation .item {
	width: 28px;
	height: 32px;
	position: relative;
	background-repeat: no-repeat;
	background-size: 28px 28px;
	background-position: center;
}

#footer .operation .item.likes {
	background-image: url(../image/community/zan@3x.png);
	background-position: left;
}

#footer .operation .item .num {
	position: absolute;
	top: 0;
	left: 19px;
	width: 38px;
	height: 14px;
	line-height: 14px;
	color: #fff;
	text-align: center;
	font-size: 10px;
	background: #FF6026;
	border-radius: 100px;
}

#footer .operation .item .num:after {
	content: "";
	width: 0;
	height: 0;
	display: block;
	border: 4px solid transparent;
	border-top: 4px solid #FF6026;
	position: absolute;
	bottom: -5px;
	left: 4px;
}

#footer .operation .item.heart {
	width: 18px;
	background-image: url(../image/mine/shoucang@2x.png);
	background-size: 18px 18px;
}

#footer .operation .item.money {
	width: 18px;
	background-image: url(../image/mine/htd1@2x.png);
	background-size: 18px 18px;
	background-position: center;
}

